{
    "_version": "1.65.0",
    "sap.app": {
        "id": "com.logaligroup.invoices",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.2",
            "toolsId": "157e5f92-56f6-4850-8cfb-840cceb308b7"
        },
        "dataSources": {
            "myNorthwind": {
                "type": "OData",
                "uri": "/v2/northwind/northwind.svc/",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.119.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.logaligroup.invoices.i18n.i18n"
                }
            },
            "invoices": {
                "type":"sap.ui.model.json.JSONModel",
                "uri": "model/Invoices.json"
            },
            "northwind": {
                "dataSource": "myNorthwind",
                "preload": true,
                "settings": {
                    "useBatch": false
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "com.logaligroup.invoices.view",
                "async": true,
                "viewPath": "com.logaligroup.invoices.view"
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": ":?query:",
                    "target": [
                        "TargetApp"
                    ]
                },
                {
                    "name":"RouteDetails",
                    "pattern": "detail({path})",
                    "target":"TargetDetails"
                }
            ],
            "targets": {
                "TargetApp": {
                    "viewName": "App"
                },
                "TargetDetails": {
                    "viewName": "Details"
                }
            }
        },
        "rootView": {
            "viewName": "com.logaligroup.invoices.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}